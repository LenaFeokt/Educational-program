<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="320.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
fx:controller="com.example.countires.controllers.ResultWindowController">
    <TableView fx:id="resultsTable" prefWidth="310" prefHeight="250" layoutX="20.0" layoutY="20.0">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="155.0" text="Имя"/>
            <TableColumn fx:id="resultColumn" prefWidth="155.0" text="Результат"/>
        </columns>
    </TableView>
    <Button fx:id="restartButton" layoutX="60.0" layoutY="280.0" mnemonicParsing="false" onAction="#restartTest" text="Запустить тест"/>
    <Button fx:id="deleteButton" layoutX="160.0" layoutY="280.0" mnemonicParsing="false" onAction="#deleteResult" text="Удалить результат"/>
</AnchorPane>